{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/style-prop-object": [0, {
      "allow": ["App"]
    }],
    "prettier/prettier": [
      "error", {
        "semi": true
      }
    ]
  }
}